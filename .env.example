# Docker Compose Environment Variables
# Copy this file to .env and customize as needed

# Required: Password for accessing the app
APP_PASSWORD=your-secure-password

# Optional: JWT token signing key (auto-generated if not set)
SECRET_KEY=

# Optional: Host port for the web interface (default: 8000)
HOST_PORT=8000

# Required: Path to your vaults directory on the host machine
VAULTS_PATH=/path/to/your/vaults
